<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.FoodController">
   <children>
      <Button fx:id="addSnacksToOrderButton" defaultButton="true" layoutX="598.0" layoutY="389.0" mnemonicParsing="false" onAction="#onAddOrderClicked" prefHeight="51.0" prefWidth="123.0" text="Add To Order">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Button fx:id="viewOrderButton" layoutX="751.0" layoutY="540.0" mnemonicParsing="false" onAction="#onViewOrderButtonClicked" prefHeight="51.0" prefWidth="168.0" text="View Order">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="previousPageButton" layoutX="60.0" layoutY="546.0" mnemonicParsing="false" onAction="#onPreviousButtonClicked" prefHeight="51.0" prefWidth="123.0" text="Previous" />
      <Label fx:id="foodStatusLabel" layoutX="10.0" layoutY="603.0" prefHeight="26.0" prefWidth="317.0" />
      <Label layoutX="758.0" layoutY="601.0" prefHeight="18.0" prefWidth="74.0" text="Employee ID: " />
      <ToggleButton fx:id="croissantButton" layoutX="70.0" layoutY="201.0" mnemonicParsing="false" prefHeight="62.0" prefWidth="177.0" text="Croissant">
         <toggleGroup>
            <ToggleGroup fx:id="selection" />
         </toggleGroup>
         <font>
            <Font size="20.0" />
         </font>
      </ToggleButton>
      <ToggleButton fx:id="bagelButton" layoutX="406.0" layoutY="201.0" mnemonicParsing="false" prefHeight="62.0" prefWidth="177.0" text="Bagel" toggleGroup="$selection">
         <font>
            <Font size="20.0" />
         </font></ToggleButton>
      <ToggleButton fx:id="cookieButton" layoutX="742.0" layoutY="201.0" mnemonicParsing="false" prefHeight="62.0" prefWidth="168.0" text="Cookie" toggleGroup="$selection">
         <font>
            <Font size="20.0" />
         </font></ToggleButton>
      <Label fx:id="employeeId" layoutX="841.0" layoutY="601.0" prefHeight="18.0" prefWidth="74.0" />
      <ComboBox fx:id="foodQuantity" layoutX="254.0" layoutY="389.0" prefHeight="51.0" prefWidth="150.0" promptText="Quantity" />
      <Label alignment="CENTER" layoutX="148.0" layoutY="22.0" prefHeight="108.0" prefWidth="696.0" text="Food">
         <font>
            <Font name="Nirmala UI" size="48.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
