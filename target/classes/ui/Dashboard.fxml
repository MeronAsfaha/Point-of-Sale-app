<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.DashboardController">
   <children>
      <Button fx:id="beveragesButton" layoutX="203.0" layoutY="206.0" mnemonicParsing="false" onAction="#switchToBeveragesWindow" prefHeight="75.0" prefWidth="172.0" text="Beverages">
         <font>
            <Font name="System Bold" size="20.0" />
         </font></Button>
      <Button fx:id="snacksButton" layoutX="619.0" layoutY="207.0" mnemonicParsing="false" onAction="#switchToFoodWindow" prefHeight="75.0" prefWidth="165.0" text="Food">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
      <Label fx:id="dashboardStatusLabel" layoutX="9.0" layoutY="568.0" prefHeight="27.0" prefWidth="336.0" />
      <Button fx:id="viewOrderButton" defaultButton="true" layoutX="424.0" layoutY="366.0" mnemonicParsing="false" onAction="#onViewOrderButtonClicked" prefHeight="64.0" prefWidth="152.0" text="View Order">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="logOutButton" layoutX="810.0" layoutY="499.0" mnemonicParsing="false" onAction="#onLogOutClicked" prefHeight="56.0" prefWidth="142.0" text="Log Out" />
      <Label alignment="CENTER" layoutX="780.0" layoutY="559.0" prefHeight="26.0" prefWidth="82.0" text="Employee ID: " />
      <Label fx:id="employeeId" alignment="CENTER" layoutX="873.0" layoutY="560.0" prefHeight="26.0" prefWidth="63.0" />
      <Button fx:id="viewPastOrdersButton" defaultButton="true" layoutX="61.0" layoutY="502.0" mnemonicParsing="false" onAction="#onViewPastOrdersButtonClicked" prefHeight="56.0" prefWidth="139.0" text="View Past Orders">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label alignment="CENTER" layoutX="148.0" layoutY="22.0" prefHeight="108.0" prefWidth="696.0" text="Dashboard">
         <font>
            <Font name="Nirmala UI" size="48.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
